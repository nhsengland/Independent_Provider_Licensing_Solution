{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "LicensingGateway": "Server=(localdb)\\MSSQLLocalDB;Database=LicensingGateway;Trusted_Connection=True;TrustServerCertificate=True",
        "LicenceHolder": "Server=(localdb)\\MSSQLLocalDB;Database=LicenseHolder;Trusted_Connection=True;TrustServerCertificate=True"
    },
    "SQL": {
        "DatabaseErrorNumbersForRetry": "-2,40197,40501,11001",
        "MaxRetryCount": 5,
        "MaxRetryDelayInSeconds": 5
    },
    "QueueClientConfiguration": {
        "EmailNotificationQueueName": "email-notification",
        "StorageAccountName": "UseDevelopmentStorage=true"
    },
    "Session": {
        "IdleTimeoutFromMinutes": 20
    },
    "Okta": {
        "Domain": "https://***.oktapreview.com",
        "ClientId": "ClientId",
        "ClientSecret": "ClientSecret"
    },
    "HomeControllerPageConfiguration": {
        "LicenceHolderApplicationURL": "https://localhost:7150/",
        "RegisterOfLicensedProvidersURL": "https://www.england.nhs.uk/publication/nhs-provider-directory-and-registers-of-licensed-healthcare-providers/",
        "LicenceComplianceURL": "https://www.england.nhs.uk/licensing-and-oversight-of-independent-providers/guidance-and-support/",
        "MonitoringAndEnforcementURL": "https://www.england.nhs.uk/licensing-and-oversight-of-independent-providers/monitoring-and-enforcement/"
    }
}
