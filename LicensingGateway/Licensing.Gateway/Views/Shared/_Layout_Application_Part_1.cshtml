<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">

    <title>NHS England provider licence application: part one - @ViewBag.Title</title>

    <link rel="preload" as="font" href="https://assets.nhs.uk/fonts/FrutigerLTW01-55Roman.woff2" type="font/woff2" crossorigin>
    <link rel="preload" as="font" href="https://assets.nhs.uk/fonts/FrutigerLTW01-65Bold.woff2" type="font/woff2" crossorigin>
    <link rel="preconnect  dns-prefetch" href="https://www.nhs.uk/">
    <link rel="preconnect  dns-prefetch" href="https://assets.nhs.uk/" crossorigin>

    <link rel="stylesheet" href="~/css/nhsuk-8.1.0.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/session.refresh.css" asp-append-version="true" />

    <!-- Favicons -->
    <link rel="shortcut icon" href="~/assets/favicons/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/assets/favicons/apple-touch-icon-180x180.png">
    <link rel="mask-icon" href="~/assets/favicons/favicon.svg" color="#005eb8">
    <link rel="icon" sizes="192x192" href="~/assets/favicons/favicon-192x192.png">
    <meta name="msapplication-TileImage" content="~/assets/favicons/mediumtile-144x144.png">
    <meta name="msapplication-TileColor" content="#005eb8">
    <meta name="msapplication-square70x70logo" content="~/assets/favicons/smalltile-70x70.png">
    <meta name="msapplication-square150x150logo" content="~/assets/favicons/mediumtile-150x150.png">
    <meta name="msapplication-wide310x150logo" content="~/assets/favicons/widetile-310x150.png">
    <meta name="msapplication-square310x310logo" content="~/assets/favicons/largetile-310x310.png">
</head>

<body class="govuk-frontend-supported">
    <script>
        document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');
    </script>

    <a class="nhsuk-skip-link" href="#maincontent">Skip to main content</a>

    <partial name="_Header" />

    <div class="nhsuk-width-container ">

        <div class="nhsuk-u-margin-bottom-5">
        </div>

        <section class="nhsuk-section">
            <div class="nhsuk-width-container app-width-container">
                <div class="nhsuk-grid-row">
                    <div class="nhsuk-grid-column-two-thirds">
                        <main class="nhsuk-main-wrapper nhsuk-main-wrapper--s" id="maincontent" role="main">
                        @RenderBody()
                        </main>
                    </div>
                </div>
            </div>
        </section>
    </div>

    <partial name="_Footer" />

    <!-- Scripts -->
    <script src="~/js/nhsuk-8.1.0.min.js" defer></script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

    <meta name="hmrc-timeout-dialog"
          content="hmrc-timeout-dialog"
          data-language="en"
          data-timeout="1200"
          data-countdown="300"
          data-keep-alive-url="?continue=true"
          data-sign-out-url="/application-part-one/session-timeout"
          data-timeout-url=""
          data-title="Your session is about to expire"
          data-message="When your session expires, we will automatically redirect you to the session timeout page, in"
          data-message-suffix=""
          data-keep-alive-button-text="Keep session alive"
          data-sign-out-button-text=""
          data-synchronise-tabs=""
          data-hide-sign-out-button="true" />
    <script src="~/js/session.refresh.js" defer></script>
</body>
</html>
