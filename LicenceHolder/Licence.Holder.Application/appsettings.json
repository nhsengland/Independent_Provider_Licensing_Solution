{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LicenseHolderGateway": "Server=localhost;Database=LicenseHolder;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "Okta": {
    "Domain": "https://***.oktapreview.com",
    "ClientId": "ClientId",
    "ClientSecret": "ClientSecret"
  },
  "Notifications": {
    "PageSize": 10
  },
  "Session": {
    "IdleTimeoutFromMinutes": 20
  },
  "SQL": {
    "DatabaseErrorNumbersForRetry": "-2,40197,40501,11001",
    "MaxRetryCount": 5,
    "MaxRetryDelayInSeconds": 5
  },
  "QueueClientConfiguration": {
    "CreateOktaUserQueueName": "create-okta-user",
    "RemoveOktaUserFromGroupQueueName": "remove-okta-user-from-group",
    "StorageAccountName": "UseDevelopmentStorage=true",
    "AutomationStorageAccountName": "UseDevelopmentStorage=true",
    "AutomationRefreshSharepointPermissionQueueName": "logicapp-07"
  }
}